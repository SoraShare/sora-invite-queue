# Environment Variables for Sora Queue UI

# Local Development Configuration
# Set to true when using local Supabase instance (supabase start)
# Example values for development:
# VITE_SUPABASE_URL=https://your-project-ref.supabase.co
# VITE_SUPABASE_ANON_KEY=eyJhbGciOiJIUzI1NiIsInR5cCI6IkpXVCJ9...
VITE_USE_LOCAL_SUPABASE=false
VITE_SUPABASE_URL=your_supabase_project_url
VITE_SUPABASE_ANON_KEY="your_supabase_anon_key or sb_publishable_key_for_local"

# Application Configuration
VITE_APP_URL=http://localhost:3000
VITE_APP_ENV=development

# Queue Auto-Refresh Configuration
# Enable/disable automatic queue status refreshing (useful to disable in development to save resources)
# Queue refresh interval in milliseconds (default: 30000 = 30 seconds)
VITE_ENABLE_QUEUE_AUTO_REFRESH=false
VITE_QUEUE_REFRESH_INTERVAL=30000

# Feature Flags
# Enable or disable specific features for testing or gradual rollout
VITE_COMMUNITY_RETURN_RATE_ENABLED=false
VITE_QUEUE_STATS_ENABLED=false

# Debug Configuration
# Show debug panel with auth state information (useful for development)
VITE_SHOW_AUTH_DEBUG=false